<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Transfer">
    <TableView fx:id="transferOneTable" layoutX="14.0" layoutY="14.0" prefHeight="288.0" prefWidth="354.0">
        <columns>
            <TableColumn prefWidth="75" text="Name">
                <cellValueFactory>
                    <PropertyValueFactory property="name" />
                </cellValueFactory>
            </TableColumn>
        </columns>
        <columns>
            <TableColumn prefWidth="75" text="Price">
                <cellValueFactory>
                    <PropertyValueFactory property="price" />
                </cellValueFactory>
            </TableColumn>
        </columns>
        <columns>
            <TableColumn prefWidth="75" text="Quantiti">
                <cellValueFactory>
                    <PropertyValueFactory property="quantiti" />
                </cellValueFactory>
            </TableColumn>
        </columns>
    </TableView>
    <TableView fx:id="transferTwoTable" layoutX="471.0" layoutY="14.0" prefHeight="288.0" prefWidth="354.0">
        <columns>
            <TableColumn prefWidth="75" text="Name">
                <cellValueFactory>
                    <PropertyValueFactory property="name" />
                </cellValueFactory>
            </TableColumn>
        </columns>
        <columns>
            <TableColumn prefWidth="75" text="Price">
                <cellValueFactory>
                    <PropertyValueFactory property="price" />
                </cellValueFactory>
            </TableColumn>
        </columns>
        <columns>
            <TableColumn prefWidth="75" text="Quantiti">
                <cellValueFactory>
                    <PropertyValueFactory property="quantiti" />
                </cellValueFactory>
            </TableColumn>
        </columns>
    </TableView>
   <Button fx:id="transferButton" layoutX="384.0" layoutY="113.0" mnemonicParsing="false" onAction="#transferItemIn" text="Transfer In" />
   <TextField fx:id="transferQuantiti" layoutX="383.0" layoutY="68.0" prefHeight="25.0" prefWidth="73.0" />
   <Button fx:id="goBack" layoutX="27.0" layoutY="324.0" mnemonicParsing="false" onAction="#showMenu" text="Go Back" />
   <Button layoutX="390.0" layoutY="175.0" mnemonicParsing="false" onAction="#removeItem" text="Remove" />
   <Button layoutX="375.0" layoutY="146.0" mnemonicParsing="false" onAction="#transferItemOut" text="Transfero Out" />

</AnchorPane>
